load NM_Test2_FS_Mag_Phase.mat

test = TestClass(Mag,Phase,Mask);
test.MyInfo.Vox = [1 1 3] * 1e-3;
test.MyInfo.MultiStart = true;
test.EchoIndexes = [1, 60];

tic
%Threshold = 0.01;
%ResThreshold = 0.2;
test = CalcLFGC(test);
test = Calc_SC(test,2);
test = Calc_2PM(test);
disp('Saving results...')
test.Description = 'Calculating MWF from LFGC! 5Param 2PM! First 60Echos!';
test.RunTime = toc;
data = GetAllData(test);
save('NM_Test2_FS_60E_2PM_MS','data');
disp('Done!')
