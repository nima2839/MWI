load NM_Test2_FS_Mag_Phase.mat

test = TestClass(Mag,Phase,Mask);
test.MyInfo.Vox = [1 1 3] * 1e-3;
% mytest.EchoIndexes = [1, 96];

tic
%Threshold = 0.01;
%ResThreshold = 0.2;
test = CalcLFGC(test);
test = Calc_SC(test,2);
test = Calc_3PM(test);
disp('Saving results...')
test.Description = 'Calculating MWF from LFGC! 8Param 3PM! First 60Echos!';
test.RunTime = toc;
save('NM_Test2_FS_96E','test');
disp('Done!')
